{
  "conditionalSettings": [
    {
      "workflows": [
        "CI/CD"
      ],
      "settings": {
        "workflowConcurrency": [
          "group: ${{ github.workflow }}-${{ github.ref }}",
          "cancel-in-progress: true"
        ]
      }
    }
  ],
  "templateUrl": "https://github.com/businesscentralapps/tmpaTN1kR-PTE@main",
  "type": "PTE",
  "incrementalBuilds": {
    "onPush": true
  },
  "doNotPublishApps": true,
  "githubRunner": "ubuntu-latest",
  "UseProjectDependencies": true,
  "$schema": "https://raw.githubusercontent.com/microsoft/AL-Go/Actions/646f103480ad85b9a8638587c497b1448472b8ad/.Modules/settings.schema.json",
  "githubRunnerShell": "pwsh",
  "useCompilerFolder": true,
  "useApproximateVersion": true,
  "MicrosoftTelemetryConnectionString": "",
  "runs-on": "ubuntu-latest",
  "shell": "pwsh"
}
